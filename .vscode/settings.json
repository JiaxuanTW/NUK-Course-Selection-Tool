{
    "editor.fontSize": 16,
    "terminal.integrated.fontFamily": "Cascadia Code PL",
    "cSpell.words": [
        "borderless",
        "ccircle",
        "cpath",
        "csvg",
        "datatable",
        "datetime",
        "focusring",
        "hexcode",
        "readonly",
        "scrollable",
        "scrollbar",
        "topnav",
        "topnavbar",
        "unstyled"
    ],
    "appService.zipIgnorePattern": [
        "__pycache__{,/**}",
        "*.py[cod]",
        "*$py.class",
        ".Python{,/**}",
        "build{,/**}",
        "develop-eggs{,/**}",
        "dist{,/**}",
        "downloads{,/**}",
        "eggs{,/**}",
        ".eggs{,/**}",
        "lib{,/**}",
        "lib64{,/**}",
        "parts{,/**}",
        "sdist{,/**}",
        "var{,/**}",
        "wheels{,/**}",
        "share/python-wheels{,/**}",
        "*.egg-info{,/**}",
        ".installed.cfg",
        "*.egg",
        "MANIFEST",
        ".env{,/**}",
        ".venv{,/**}",
        "env{,/**}",
        "venv{,/**}",
        "ENV{,/**}",
        "env.bak{,/**}",
        "venv.bak{,/**}",
        ".vscode{,/**}"
    ]
}